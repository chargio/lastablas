<tr>
  <td>
    <%= link_to user.id, user_path(user) %>
  </td>
  <td>
    <%= link_to user.email, user_path(user) %>
  </td>
  <td>
    <%= user.first_name %>
  </td>
  <td>
    <%= user.last_name %>
  </td>
  <td>
    <%= user.offsprings.count %>
  </td>
  <td>
    <%= user.phone %>
  </td>
  <td>
    <%= l user.created_at %>
  </td>
  <td>
    <%= l user.updated_at %>
  </td>
</tr>
